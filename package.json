{
  "name": "@hongminhee/localtunnel",
  "version": "0.4.0",
  "description": "A simple wrapper around the local tunneling services",
  "keywords": ["localtunnel", "tunnel"],
  "license": "MIT",
  "author": {
    "name": "Hong Minhee",
    "email": "hong@minhee.org",
    "url": "https://hongminhee.org/"
  },
  "homepage": "https://github.com/dahlia/localtunnel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dahlia/localtunnel.git"
  },
  "bugs": {
    "url": "https://github.com/dahlia/localtunnel/issues"
  },
  "funding": [
    "https://github.com/sponsors/dahlia"
